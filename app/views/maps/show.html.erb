<%
  #caching causing issue where background not cleared
  #cache [create_cache_key_prefix, "map-rendered"], :expires_in => 1.day do %>
  <%= content_for :head do %>
    <style>
      body {
        background: none;
      }
    </style>
  <% end %>
  <iframe style="width: 100%;height: 100%;position: absolute;" src="<%=@map_url%>" frameborder="0" allowfullscreen></iframe>
<% #end %>
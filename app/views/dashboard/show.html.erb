<div class="container-fluid">

  <div id="user-info" class="user-info row d-flex justify-content-center align-items-center">

    <div class="col-4">

      <% if (false) %>
      <table id="user-info-table" class="table table-sm table-bordered">
        <caption>Please Verify Your Profile Information Is Current</caption>
        <thead>
        <tr>
          <th scope="col">MY PROFILE</th>
          <th scope="col"><button>EDIT</button></th>
        </tr>
        </thead>
        <tbody>
        <tr>
          <th scope="row">First Name</th>
          <td><%= @current_user.first_name %></td>
        </tr>
        <tr>
          <th scope="row">Last Name</th>
          <td><%= @current_user.last_name %></td>
        </tr>
        <tr>
          <th scope="row">Email</th>
          <td><%= @current_user.email %></td>
        </tr>

        <tr>
          <th scope="row">Gender</th>
          <td><%= @current_user.gender %></td>
        </tr>

        </tbody>
      </table>
      <% end %>

      <h1 class="text-uppercase">PROFILE FOR <%= current_user.name %></h1>
      <caption>Please Verify Your Profile Information Is Current</caption>
      <br>
      <div id="user-form-alert" class="alert invisible" role="alert">TEST TEST TEST</div>
      <%= render :partial => "users/form" %>
    </div>
  </div>
</div>
<% if(!@current_user.course_enrollments.food_etalk.completed.empty?) %>
  <div id="food-etalk-lessons-completed" class="d-flex flex-column pb-4">
    <div class="media">
      <div class="media-body">
        <span><%= image_tag 'food-etalk-logo-med.png', class: 'align-self-top', alt: 'Food eTalk Logo', style: 'width: 6rem; height: auto;' %></span>
        <span class="pb-2 lead">
          <%= t("dashboard.my_courses.completed.food_talk_completed_lesson") %>
        </span>
      </div>
    </div>
    <div class="pt-2 food-etalk-completed-grid">
      <% LearningModules::FOOD_ETALK.each do |lesson| %>
        <% if LearningModules.user_completed_module?(@current_user, lesson) %>
          <%= render partial: "include_learning_module_media_object", locals: {lesson: lesson, my_css: 'modules-grid'} %>
        <% end %>
      <% end %>
    </div>
  </div>
<% end %>